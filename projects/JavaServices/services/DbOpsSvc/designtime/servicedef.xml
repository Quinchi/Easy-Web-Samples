<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<service xmlns="http://www.activegrid.com/namespaces/ServiceDefinitions/1.0" id="DbOpsSvc" class="com.custpurchasedb.DbOpsSvc" CRUDService="false" type="JavaService" springFile="DbOpsSvc.spring.xml">
    <operation name="setCustpurchaseDB">
        <parameter name="custpurchaseDB" typeRef="com.custpurchasedb.CustpurchaseDB" isList="false"/>
    </operation>
    <operation name="updateCustomer">
        <parameter name="customer" typeRef="com.custpurchasedb.data.Customer" isList="false"/>
        <return typeRef="com.custpurchasedb.data.Customer" isList="false"/>
    </operation>
    <dataobjects>
        <dataobject javaType="com.custpurchasedb.CustpurchaseDB" name="CustpurchaseDB" supportsQuickData="false">
            <element name="dataServiceManager" typeRef="com.wavemaker.runtime.data.DataServiceManager" isList="false" allowNull="false"/>
            <element name="taskManager" typeRef="com.wavemaker.runtime.data.TaskManager" isList="false" allowNull="false"/>
        </dataobject>
    </dataobjects>
</service>
